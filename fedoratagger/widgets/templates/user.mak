<div class="userinfo">
  <table>
    <tr>
      <td>
        <div class="photo-64" style="display: inline-block !important;">
          <img src="${w.avatar_link(s=64)}"></img>
        </div>
      </td>
      <td style="width:4px;"></td>
      <td>
        <p>${str(w.formatted_name)}</p>
        <p>${str(w.total_votes)} votes cast.</p>
        <p>Rank: ${str(w.rank)}.</p>
      </td>
    </tr>
  </table>
</div>
